{
  "project": {
    "version": "1.0.0-beta.26",
    "name": "dev"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "../dist",
      "assets": [
        "assets",
        "favicon.ico",
        "placeholders",
        "img.jpg",
        "logo.png"
      ],
      "index": "index.html",
      "main": "main.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "app",
      "mobile": false,
      "styles": [
        "../node_modules/bulma/css/bulma.css",
        "../node_modules/font-awesome/css/font-awesome.min.css",
        "assets/dropdown.css",
        "styles.css"
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.min.js",
        "../node_modules/base64-js/base64js.min.js",
        "assets/dropdown.js"
      ],
      "environments": {
        "source": "environments/environment.ts",
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
